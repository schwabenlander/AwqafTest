@model IEnumerable<AwqafTest.Models.FiscalYear>

@{
    ViewData["Title"] = "Fiscal Years";
}

<h1>@ViewData["Title"]</h1>

<p>
    <a asp-action="Create">Add New Fiscal Year</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FiscalYearId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.YearDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsCurrent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsOpen)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FiscalYearId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YearDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsCurrent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsOpen)
        </td>
        <td>
            <a asp-action="Delete" asp-route-id="@item.FiscalYearId">Delete</a>
        </td>
    </tr>
}
    </tbody>
</table>
